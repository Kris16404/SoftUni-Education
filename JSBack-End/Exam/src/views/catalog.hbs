<section id='viewCatalog' class='background-img'>
  <!--If there are one or more electronics-->
  {{#if electronics}}
    <div class='added-electronics'>
      {{#each electronics}}
        <a href='/details/{{this._id}}' class='added-electronics-in-market'>
          <img src='{{this.image}}' class='picture-added-electronics' />
          <h3>{{this.name}}</h3>
          <span>Price: ${{this.price}}</span>
        </a>
      {{/each}}
    </div>
  {{else}}
    <!--If there are no electronics in catalog yet-->
    <div class='guest'>
      There are no electronics found...
    </div>
  {{/if}}
</section>